  "trigger": [
    {
      "event": "%s%Diagnostics::UPTIME%/s%",
      "actions": [
        {
          "objectType": "WriteToSymbol",
          "active": true,
          "symbolExpression": "%s%PLC1.GVL_HMI_GLOBAL.sUserGroup%/s%",
          "value": {
            "objectType": "Symbol",
            "valueType": "tchmi:general#/definitions/String",
            "symbolExpression": "%s%GetCurrentUser.groups%/s%"
          },
          "asyncWait": true
        },
        {
          "objectType": "WriteToSymbol",
          "active": true,
          "symbolExpression": "%s%PLC1.GVL_HMI_GLOBAL.sUserName%/s%",
          "value": {
            "objectType": "Symbol",
            "valueType": "tchmi:general#/definitions/String",
            "symbolExpression": "%s%GetCurrentUser::name%/s%"
          },
          "asyncWait": true
        },
        {
          "objectType": "Condition",
          "active": true,
          "parts": [
            {
              "if": [
                {
                  "compare1": {
                    "objectType": "Symbol",
                    "valueType": "tchmi:general#/definitions/BOOL",
                    "symbolExpression": "%s%PLC1.GVL_HMI_GLOBAL.bLogOut%/s%"
                  },
                  "compare2": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/BOOL",
                    "value": true
                  },
                  "compareOperator": "==",
                  "logic": null
                }
              ],
              "then": [
                {
                  "objectType": "Function",
                  "active": true,
                  "fn": "TcHmi.Functions.Beckhoff.Logout",
                  "fnParams": [
                    {
                      "objectType": "StaticValue",
                      "valueType": "tchmi:general#/definitions/Boolean",
                      "value": true
                    }
                  ],
                  "asyncWait": true
                },
                {
                  "objectType": "WriteToSymbol",
                  "active": true,
                  "symbolExpression": "%s%PLC1.GVL_HMI_GLOBAL.bUserLogedIntSignal%/s%",
                  "value": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/BOOL",
                    "value": false
                  },
                  "asyncWait": true
                }
              ]
            },
            {
              "else": []
            }
          ],
          "asyncWait": true
        },
        {
          "objectType": "WriteToSymbol",
          "active": true,
          "symbolExpression": "%s%PLC1.GVL_HMI_GLOBAL.nRegionMain%/s%",
          "value": {
            "objectType": "Symbol",
            "valueType": "tchmi:general#/definitions/String",
            "symbolExpression": "%i%Screen%/i%"
          },
          "asyncWait": true
        }
      ]
    }
  ],
