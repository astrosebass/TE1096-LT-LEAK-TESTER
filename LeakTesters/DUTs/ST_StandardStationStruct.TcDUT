	bNoCycleStopFault				: 	BOOL;		//No  Stop Fault On Station - Immidiat STOP Station
	bNoImmediateFault				: 	BOOL;		//No Immediate Fault On Station - Immidiat STOP Machine
	
	
	bStationManualRequestHMI		:	BOOL;		//Sequence Cycle Requested From HMI
	bStationInManualMode			:	BOOL;		//Station Sequence Manage By Operator

	bStationInStandByMode			:	BOOL;		//Sequence In Stand By State
	bStationFreezedByManager		: 	BOOL;		//Sequence Cycle Freezed (Suspend)  By Manager
	
	eStationState					: 	E_StationState; // state of sequence
	
	bPartOK							:	BOOL;		// 1-Cycle succeed
	bPartReject                     :   BOOL;       // 1-Cycle failed

	
	bStationWaiting                 :   BOOL; // Station in the  positiont waiting for work done
	bStationCycleCondition          :   BOOL; // Station in cycle move
