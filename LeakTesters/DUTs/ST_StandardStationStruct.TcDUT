  <DUT Name="ST_StandardStationStruct" Id="{7dda221a-7a06-46d2-b131-92bd1a558b98}">
    <Declaration><![CDATA[TYPE ST_StandardStationStruct :
STRUCT
	
		// Station Manager => Master Control
	nState 							: 	DINT := 100;//Station Cycle State
	bStationWaitOnInit              :   BOOL;       //Station Waiting For Initialisation Request
	bStationWaitOnRequest			:	BOOL;		//Station In Valide State Avaiting For Request Command
	bStationInProgress				: 	BOOL;		//Station In Progress With Cycle
	bStationInProgressInit          :   BOOL;       //Station In Progress With Imotialisation     
	bStationComplete				: 	BOOL;		//Station Complete Cycle - Flag Set After Every Cycle Finsh
	bStationCycleStopped			: 	BOOL;
	bStationHomed					: 	BOOL;		//Station Homed - All Station Functions Are Homed 
	bStationDone					:	BOOL;		//Sequence Finished With Success - Flag Set For One PLC Cycle
	bStationNotMoving				: 	BOOL;		//Station Parts Are Not Moving
	bStationInDwell					: 	BOOL;		//Station Waiting For External Action By Operator
	bStationAcknowledge             :   BOOL;       //Station Acknowledge request from Master 
	bStationInitDone                :   BOOL;       //Station Initialisation Progress Done
